InputQuery (
    query: r#"
{
  GitlabRepos(language: "python") {
      url @output
      id @output
      name @output
      description @output
      files(ref: "main") {
        path @output @filter(op: "regex", value: ["$path_regex"])
        content @output
      }
    }
}"#,
    args: {
        // "url_regex": String("test"),
        "path_regex": String("requirements.txt"),
    },
)

